{
    "name": "hoserver-admin",
    "version": "1.0.0",
    "description": "An out-of-box UI solution for enterprise applications",
    "scripts": {
        "analyze": "cross-env ANALYZE=1 umi build",
        "build": "umi build",
        "deploy": "npm run site && npm run gh-pages",
        "fetch:blocks": "pro fetch-blocks && npm run prettier",
        "format-imports": "cross-env import-sort --write '**/*.{js,jsx,ts,tsx}'",
        "gh-pages": "cp CNAME ./dist/ && gh-pages -d dist",
        "i18n-remove": "pro i18n-remove --locale=zh-CN --write",
        "lint": "npm run lint:js && npm run lint:style && npm run lint:prettier",
        "lint-staged": "lint-staged",
        "lint-staged:js": "eslint --ext .js,.jsx,.ts,.tsx ",
        "lint:fix": "eslint --fix --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src && npm run lint:style",
        "lint:js": "eslint --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src",
        "lint:prettier": "check-prettier lint",
        "lint:style": "stylelint --fix \"src/**/*.less\" --syntax less",
        "prettier": "prettier -c --write \"**/*\"",
        "start": "cross-env UMI_UI=none MOCK=none umi dev",
        "tsc": "tsc",
        "ui": "umi ui"
    },
    "lint-staged": {
        "**/*.less": "stylelint --syntax less",
        "**/*.{js,jsx,ts,tsx}": "npm run lint-staged:js",
        "**/*.{js,jsx,tsx,ts,less,md,json}": [
            "prettier --write",
            "git add"
        ]
    },
    "browserslist": [
        "> 1%",
        "last 2 versions",
        "not ie <= 10"
    ],
    "dependencies": {
        "@ant-design/compatible": "^1.0.8",
        "@ant-design/icons": "^4.6.2",
        "@ant-design/pro-layout": "4.11.4",
        "@ant-design/pro-table": "^2.45.0",
        "@hosoft/hos-admin-common": "^1.0.2",
        "@hosoft/hos-plugin-auto-crud-page": "^1.0.2",
        "@hosoft/hos-plugin-batch-import-export": "^1.0.2",
        "@hosoft/hos-plugin-cloud-storage": "^1.0.2",
        "@hosoft/hos-plugin-doc-gen": "^1.0.2",
        "@hosoft/hos-plugin-payment": "^1.0.2",
        "@hosoft/hos-plugin-sdk-gen": "^1.0.2",
        "@hosoft/hos-plugin-sms": "^1.0.2",
        "@hosoft/hos-plugin-social": "^1.0.2",
        "@types/lodash.debounce": "^4.0.6",
        "@types/lodash.isequal": "^4.5.5",
        "@types/react-router": "^5.1.16",
        "antd": "^4.16.8",
        "classnames": "^2.3.1",
        "dva": "^2.4.1",
        "eslint-plugin-simple-import-sort": "^7.0.0",
        "file-saver": "^2.0.5",
        "lodash": "^4.17.11",
        "lodash-decorators": "^6.0.0",
        "lodash.debounce": "^4.0.8",
        "lodash.isequal": "^4.5.0",
        "mockjs": "^1.1.0",
        "moment": "^2.29.1",
        "numeral": "^2.0.6",
        "nzh": "^1.0.5",
        "omit.js": "^2.0.2",
        "path-to-regexp": "6.2.0",
        "pluralize": "^8.0.0",
        "prop-types": "^15.7.2",
        "qs": "^6.10.1",
        "querystring": "^0.2.1",
        "react": "^17.0.2",
        "react-beautiful-dnd": "^13.1.0",
        "react-copy-to-clipboard": "^5.0.3",
        "react-dom": "^17.0.2",
        "react-fittext": "^1.0.0",
        "react-helmet": "^6.1.0",
        "react-json-view": "^1.21.3",
        "react-router": "^5.2.0",
        "redux": "^4.1.0",
        "umi": "^2.8.7",
        "umi-plugin-react": "^1.9.5",
        "use-merge-value": "^1.0.2",
        "umi-request": "^1.2.17"
    },
    "devDependencies": {
        "@ant-design/pro-cli": "^2.1.4",
        "@types/classnames": "^2.3.1",
        "@types/express": "^4.17.13",
        "@types/history": "^4.7.9",
        "@types/jest": "^26.0.24",
        "@types/lodash": "^4.14.171",
        "@types/qs": "^6.9.7",
        "@types/react": "^17.0.15",
        "@types/react-dom": "^17.0.9",
        "@types/react-helmet": "^6.1.2",
        "@umijs/fabric": "2.0.0",
        "chalk": "^4.1.1",
        "check-prettier": "^1.0.3",
        "cross-env": "^7.0.3",
        "cross-port-killer": "^1.3.0",
        "enzyme": "^3.11.0",
        "express": "^4.17.1",
        "gh-pages": "^3.2.3",
        "husky": "^7.0.1",
        "import-sort-cli": "^6.0.0",
        "import-sort-parser-babylon": "^6.0.0",
        "import-sort-parser-typescript": "^6.0.0",
        "import-sort-style-module": "^6.0.0",
        "jsdom-global": "^3.0.2",
        "lint-staged": "^11.1.1",
        "node-fetch": "^2.6.1",
        "prettier": "^2.3.2",
        "pro-download": "1.0.1",
        "stylelint": "^13.13.1",
        "umi-plugin-pro": "^1.0.2",
        "umi-types": "^0.5.0"
    },
    "engines": {
        "node": ">=10.0.0"
    },
    "checkFiles": [
        "src/**/*.js*",
        "src/**/*.ts*",
        "src/**/*.less",
        "config/**/*.js*",
        "scripts/**/*.js"
    ],
    "private": true
}
